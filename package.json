{
  "name": "recipebox",
  "version": "1.0.0",
  "description": "recipe box project for codecamp",
  "engines": {
    "node": "*"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --devtool source-map",
