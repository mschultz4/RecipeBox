  "dependencies": {
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "browser": "^0.2.6",
    "express": "^4.14.0",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.14.1",
    "mongodb": "^2.2.10",
    "object-assign": "^4.1.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
